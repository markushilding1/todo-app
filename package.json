{
  "dependencies": {
    "expo": "50.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-web": "0.19.6"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@expo/metro-runtime": "3.1.3",
    "@react-native/eslint-config": "0.73.2",
    "@types/react": "18.2.45",
    "@types/react-native": "0.70.6",
    "eslint": "8.56.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "style:all:check": "npm run style:lint && npm run style:prettier:check",
    "style:all:fix": "npm run style:lint:fix && npm run style:prettier:fix",
    "style:lint": "eslint",
    "style:lint:fix": "npm run style:lint -- --fix",
    "style:prettier": "prettier '**/*.{ts,tsx,js}' --ignore-unknown",
    "style:prettier:check": "npm run style:prettier -- --check",
    "style:prettier:fix": "npm run style:prettier -- --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": "npm run style:all:fix"
  },
  "version": "1.0.0",
  "private": true,
  "name": "todo-app"
}
